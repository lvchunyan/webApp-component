/* page/component/pages/swiper/swiper.wxss */

.swiper {
  margin: 0 80rpx;
}

.swiper-item-title {
  height: 150px;
  text-align: center;
  line-height: 150px;
}

.weui {
  margin: 20rpx 0;
  border-bottom: 1px solid #d8d8d8;
  border-top: 1px solid #d8d8d8;
  background: #fff;
}

.weui-cell {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin: 10rpx 30rpx;
}

.weui-cells {
  border-bottom: 1px solid #d8d8d8;
  padding-bottom: 10rpx;
}

.weui-cell-text text {
  font-size: 17px;
}

.page {
  padding: 20rpx 80rpx;
}

.page-section {
  color: #999;
  font-size: 28rpx;
}

.page-section-text {
  float: right;
  color: #353535;
  font-size: 32rpx;
}
